<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Tabela</title>
  </head>
  <body>
    <table>
      <thead>
        <% header.forEach((col) => { %>
          <th> <%= col %> </th>
        <% }) %>
      </thead>
      <tbody>
        <% rows.forEach((row) => { %>
          <tr>
            <% row.forEach((element) => { %>
              <td> <%= element %></td>
          <% }) %>
          </tr>
        <% }) %>
      </tbody>
    </table>

  </body>
</html>
